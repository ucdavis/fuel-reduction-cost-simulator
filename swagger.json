{
  "swagger": "2.0",
  "info": {
    "description": "FRCS is used for estimating costs for fuel reduction treatments involving removal of trees of mixed sizes in the form of whole trees, logs, or chips from a forest. It was originally a spreadsheet application published by U.S. Forest Service.",
    "version": "1.2.9",
    "title": "Fuel Reduction Cost Simulator Web Service",
    "contact": {
      "email": "kkyli@ucdavis.edu"
    },
    "license": {
      "name": "MIT License",
      "url": "https://github.com/ucdavis/fuel-reduction-cost-simulator/blob/master/LICENSE"
    }
  },
  "paths": {
    "/frcs": {
      "post": {
        "tags": ["FRCS"],
        "summary": "Get FRCS Outputs",
        "description": "| Limits | Ground-Based Mech WT | Ground-Based CTL | Ground-Based Manual WT | Ground-Based Manual Log | Cable Manual WT/Log | Cable Manual WT | Cable Manual Log | Cable CTL | Helicopter Manual Log | Helicopter CTL |\n|-|:-:|:-:|:-:|:-:|:-:|:-:|:-:|:-:|:-:|:-:|\n| TreeVol Max CT, ft3 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 |\n| TreeVol Max SLT, ft3 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 | 80 |\n| TreeVol Max LLT, ft3 | 250 |  | 250 | 250 | 250 | 250 | 250 |  | 250 |  |\n| Max LLT/ac |  | 10 |  |  |  |  |  | 10 |  | 10 |\n| Max LLT as % of ALT |  | 10 |  |  |  |  |  | 10 |  | 10 |\n| Max slope, % | 40 | 40 | 40 | 40 | 100 | 100 | 100 | 40 | 100 | 40 |\n| Max Yarding Dist, ft |  |  |  |  | 1300 | 1300 | 1300 | 1300 | 10000 | 10000 |\n",
        "operationId": "getFrcsOutputs",
        "produces": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "$ref": "#/definitions/FrcsInputs"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/FrcsOutputs"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    }
  },
  "definitions": {
    "FrcsInputs": {
      "type": "object",
      "properties": {
        "system": {
          "type": "string",
          "example": "Cable Manual WT",
          "description": "Selected from the ten harvesting systems including four ground-based systems, four cable-yarding systems, and two helicopter systems.",
          "enum": [
            "Ground-Based Mech WT",
            "Ground-Based Manual WT",
            "Ground-Based Manual Log",
            "Ground-Based CTL",
            "Cable Manual WT/Log",
            "Cable Manual WT",
            "Cable Manual Log",
            "Cable CTL",
            "Helicopter Manual Log",
            "Helicopter CTL"
          ]
        },
        "isPartialCut": {
          "type": "boolean",
          "example": true,
          "description": "Indicate the type of harvest: if true, partial-cut; otherwise clear-cut."
        },
        "deliverToLandingDistance": {
          "type": "number",
          "format": "float",
          "example": 400,
          "description": "Distance that trees or logs are moved from the stump to be delivered to the landing. Subject to the selected harvesting system, trees or logs can be yarded, skidded, or forwarded to the landing."
        },
        "slope": {
          "type": "number",
          "format": "float",
          "example": 30,
          "description": "Average fall-line slope (%) of the harvest unit"
        },
        "elevation": {
          "type": "number",
          "format": "float",
          "example": 5000,
          "description": "Elevation (ft) is required for helicopter systems."
        },
        "includeLoadingCosts": {
          "type": "boolean",
          "example": true,
          "description": "Indicate whether to include the costs of fuel loading."
        },
        "includeMoveInCosts": {
          "type": "boolean",
          "example": true,
          "description": "Indicate whether to include the costs of moving equipment to the harvest unit."
        },
        "area": {
          "type": "number",
          "format": "float",
          "example": 1,
          "description": "Area (acres) of the harvest unit"
        },
        "moveInDistance": {
          "type": "number",
          "format": "float",
          "example": 2,
          "description": "One-way distance (miles) of moving harvesting equipment to the harvest unit"
        },
        "includeCostsCollectChipResidues": {
          "type": "boolean",
          "example": true,
          "description": "Indicate whether to include the costs of collecting and chipping residues."
        },
        "woodDensityCT": {
          "type": "number",
          "format": "float",
          "default": 60,
          "example": 60,
          "description": "Ratio (%) of chip tree green weight to green volume (pounds per cubic foot)"
        },
        "woodDensitySLT": {
          "type": "number",
          "format": "float",
          "default": 60,
          "example": 58.6235,
          "description": "Ratio (%) of small log tree green weight to green volume (pounds per cubic foot)"
        },
        "woodDensityLLT": {
          "type": "number",
          "format": "float",
          "default": 60,
          "example": 62.1225,
          "description": "Ratio (%) of large log tree green weight to green volume (pounds per cubic foot)"
        },
        "residueFractionCT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0,
          "description": "Ratio of the green weight of chip tree tops and limbs to the green weight of the tree bole"
        },
        "residueFractionSLT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0.25,
          "description": "Ratio of the green weight of small log tree tops and limbs to the green weight of the tree bole"
        },
        "residueFractionLLT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0.38,
          "description": "Ratio of the green weight of large log tree tops and limbs to the green weight of the tree bole"
        },
        "hardwoodFractionCT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0.2,
          "description": "Ratio of the green volume of hardwood to the total green volume of chip trees in the harvest unit"
        },
        "hardwoodFractionSLT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0,
          "description": "Ratio of the green volume of hardwood to the total green volume of small log trees in the harvest unit"
        },
        "hardwoodFractionLLT": {
          "type": "number",
          "format": "float",
          "default": 0,
          "example": 0,
          "description": "Ratio of the green volume of hardwood to the total green volume of large log trees in the harvest unit"
        },
        "treesPerAcreCT": {
          "type": "number",
          "format": "float",
          "example": 20,
          "description": "Number of chip trees/acre"
        },
        "volumeCT": {
          "type": "number",
          "format": "float",
          "example": 50,
          "description": "Volume of chip trees (cf/tree)"
        },
        "treesPerAcreSLT": {
          "type": "number",
          "format": "float",
          "example": 50,
          "description": "Number of small log trees/acre"
        },
        "volumeSLT": {
          "type": "number",
          "format": "float",
          "example": 70,
          "description": "Volume of small log trees (cf/tree)"
        },
        "treesPerAcreLLT": {
          "type": "number",
          "format": "float",
          "example": 5,
          "description": "Number of large log trees/acre"
        },
        "volumeLLT": {
          "type": "number",
          "format": "float",
          "example": 100,
          "description": "Volume of large log trees (cf/tree)"
        },
        "dieselFuelPrice": {
          "type": "number",
          "format": "float",
          "example": 3.61,
          "description": "Current wholesale price ($/gallon) of low-sulfur diesel fuel in the California"
        },
        "moistureContent": {
          "type": "number",
          "format": "float",
          "example": 50,
          "description": "Moisture content of biomass on a wet basis (% w.b.)"
        },
        "isBiomassSalvage": {
          "type": "boolean",
          "example": false,
          "description": "Indicate whether all trees will be utilized as feedstock for the conversion facility."
        },
        "wageFaller": {
          "type": "number",
          "format": "float",
          "example": 35.13,
          "description": "Hourly wage of faller/bucker ($/h)"
        },
        "wageOther": {
          "type": "number",
          "format": "float",
          "example": 22.07,
          "description": "Hourly wage of other workers ($/h)"
        },
        "laborBenefits": {
          "type": "number",
          "format": "float",
          "example": 35,
          "description": "Ratio (%) of benefits and overhead to wages"
        },
        "ppiCurrent": {
          "type": "number",
          "format": "float",
          "example": 284.7,
          "description": "Latest published value of the U.S. producer price index (PPI)"
        },
        "residueRecovFracWT": {
          "type": "number",
          "format": "float",
          "example": 80,
          "description": "Green weight fraction (%) of residues recovered in WT systems"
        },
        "residueRecovFracCTL": {
          "type": "number",
          "format": "float",
          "example": 50,
          "description": "Green weight fraction (%) of residues recovered in CTL systems"
        }
      },
      "required": [
        "system",
        "isPartialCut",
        "deliverToLandingDistance",
        "slope",
        "includeLoadingCosts",
        "includeMoveInCosts",
        "area",
        "moveInDistance",
        "includeCostsCollectChipResidues",
        "treesPerAcreCT",
        "volumeCT",
        "treesPerAcreSLT",
        "volumeSLT",
        "treesPerAcreLLT",
        "volumeLLT",
        "dieselFuelPrice",
        "isBiomassSalvage",
        "wageFaller",
        "wageOther",
        "laborBenefits",
        "ppiCurrent",
        "residueRecovFracWT",
        "residueRecovFracCTL"
      ],
      "xml": {
        "name": "FrcsInputs"
      }
    },
    "FrcsOutputs": {
      "type": "object",
      "properties": {
        "totalBiomass": {
          "$ref": "#/definitions/TotalBiomass"
        },
        "feedstock": {
          "$ref": "#/definitions/Feedstock"
        }
      },
      "xml": {
        "name": "FrcsOutputs"
      }
    },
    "TotalBiomass": {
      "type": "object",
      "properties": {
        "yieldPerAcre": {
          "type": "number",
          "format": "float",
          "example": 168.639975,
          "description": "Total yield (GT/acre)"
        },
        "costPerAcre": {
          "type": "number",
          "format": "float",
          "example": 3708.621951,
          "description": "Total harvest cost ($/acre)"
        },
        "costPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 74.17243903,
          "description": "Total harvest cost ($/CCF)"
        },
        "costPerGT": {
          "type": "number",
          "format": "float",
          "example": 21.99135734,
          "description": "Total harvest cost ($/GT)"
        },
        "dieselPerAcre": {
          "type": "number",
          "format": "float",
          "example": 50.99159554,
          "description": "Total diesel consumption (gal/acre)"
        },
        "dieselPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 1.0198319107071314,
          "description": "Total diesel consumption (gal/CCF)"
        },
        "gasolinePerAcre": {
          "type": "number",
          "format": "float",
          "example": 0.388986296,
          "description": "Total gasoline consumption (gal/acre)"
        },
        "gasolinePerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 0.007779725917184,
          "description": "Total gasoline consumption (gal/CCF)"
        },
        "jetFuelPerAcre": {
          "type": "number",
          "format": "float",
          "example": 0,
          "description": "Total jet fuel consumption (gal/acre)"
        },
        "jetFuelPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 0,
          "description": "Total jet fuel consumption (gal/CCF)"
        }
      },
      "xml": {
        "name": "TotalBiomass"
      }
    },
    "Feedstock": {
      "type": "object",
      "properties": {
        "yieldPerAcre": {
          "type": "number",
          "format": "float",
          "example": 50.518225,
          "description": "Feedstock yield (GT/acre)"
        },
        "costPerAcre": {
          "type": "number",
          "format": "float",
          "example": 534.2228831,
          "description": "Feedstock harvest cost ($/acre)"
        },
        "costPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 10.68445766,
          "description": "Feedstock harvest cost ($/CCF)"
        },
        "costPerGT": {
          "type": "number",
          "format": "float",
          "example": 3.167830659,
          "description": "Feedstock harvest cost ($/GT)"
        },
        "dieselPerAcre": {
          "type": "number",
          "format": "float",
          "example": 15.64078771,
          "description": "Diesel consumption associated with feedstock harvest (gal/acre)"
        },
        "dieselPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 15.64078771,
          "description": "Diesel consumption associated with feedstock harvest (gal/CCF)"
        },
        "gasolinePerAcre": {
          "type": "number",
          "format": "float",
          "example": 0.066371983,
          "description": "Gasoline consumption associated with feedstock harvest (gal/acre)"
        },
        "gasolinePerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 0.066371983,
          "description": "Gasoline consumption associated with feedstock harvest (gal/CCF)"
        },
        "jetFuelPerAcre": {
          "type": "number",
          "format": "float",
          "example": 0,
          "description": "Jet fuel consumption associated with feedstock harvest (gal/acre)"
        },
        "jetFuelPerBoleCCF": {
          "type": "number",
          "format": "float",
          "example": 0,
          "description": "Jet fuel consumption associated with feedstock harvest (gal/CCF)"
        }
      },
      "xml": {
        "name": "Feedstock"
      }
    }
  },
  "externalDocs": {
    "description": "Source code",
    "url": "https://github.com/ucdavis/fuel-reduction-cost-simulator"
  }
}
